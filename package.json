{
  "name": "space-invaders-js-v115",
  "version": "1.15.0",
  "description": "A modern HTML5 Canvas Space Invaders game with advanced features and performance optimizations",
  "main": "index.html",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "npm run serve",
    "serve": "npx http-server . -p 8080 -c-1 --cors -o",
    "serve:dev": "npx http-server . -p 3000 -c-1 --cors -o --silent",
    "build": "npm run lint && npm run test && npm run build:assets",
    "build:assets": "npm run optimize:images && npm run minify:css && npm run minify:js",
    "build:production": "npm run build && npm run validate:build",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch --coverage",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "playwright test",
    "test:performance": "jest --testPathPattern=performance --runInBand",
    "lint": "eslint . --ext .js,.mjs --fix",
    "lint:check": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "format": "prettier --write \"**/*.{js,mjs,json,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,json,css,html,md}\"",
    "validate": "npm run lint:check && npm run format:check && npm run test:ci",
    "validate:build": "npm run validate && npm run audit:security",
    "audit": "npm audit --audit-level=moderate",
    "audit:security": "npm audit --audit-level=high",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf node_modules coverage dist .nyc_output",
    "clean:cache": "npm cache clean --force",
    "reinstall": "npm run clean && npm install",
    "optimize:images": "echo 'Image optimization placeholder - implement with imagemin'",
    "minify:css": "echo 'CSS minification placeholder - implement with cssnano'",
    "minify:js": "echo 'JS minification placeholder - implement with terser'",
    "analyze:bundle": "echo 'Bundle analysis placeholder - implement with webpack-bundle-analyzer'",
    "analyze:performance": "lighthouse http://localhost:8080 --output=html --output-path=./reports/lighthouse.html",
    "dev": "concurrently \"npm run serve:dev\" \"npm run test:watch\"",
    "precommit": "npm run validate",
    "prepush": "npm run build",
    "postinstall": "npm run audit",
    "health": "node -e \"console.log('âœ… Package health check passed')\"",
    "version:patch": "npm version patch && git push --tags",
    "version:minor": "npm version minor && git push --tags",
    "version:major": "npm version major && git push --tags"
  },
  "keywords": [
    "space-invaders",
    "html5-game",
    "canvas-game",
    "javascript-game",
    "arcade-game",
    "retro-game",
    "browser-game",
    "web-game",
    "game-development",
    "es6-modules"
  ],
  "author": {
    "name": "Space Invaders Development Team",
    "email": "dev@spaceinvaders.game",
    "url": "https://github.com/spaceinvaders/space-invaders-js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaceinvaders/space-invaders-js-v115.git"
  },
  "bugs": {
    "url": "https://github.com/spaceinvaders/space-invaders-js-v115/issues"
  },
  "homepage": "https://spaceinvaders.game",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@eslint/js": "^9.0.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-security": "^2.1.0",
    "globals": "^14.0.0",
    "http-server": "^14.1.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.4.0",
    "prettier": "^3.1.0"
  },
  "dependencies": {},
  "peerDependencies": {},
  "optionalDependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "jest-canvas-mock"
    ],
    "collectCoverageFrom": [
      "js/**/*.js",
      "!js/**/*.test.js",
      "!js/**/*.spec.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json-summary"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/tests/**/*.spec.js",
      "**/__tests__/**/*.js"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "testTimeout": 10000,
    "verbose": true,
    "bail": false,
    "maxWorkers": "50%"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "18",
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not dead"
            ]
          },
          "modules": "auto",
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ]
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "bracketSameLine": false
  },
  "config": {
    "port": 8080,
    "host": "localhost",
    "ssl": false,
    "cors": true,
    "cache": false
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/spaceinvaders"
  },
  "files": [
    "index.html",
    "js/",
    "css/",
    "assets/",
    "README.md",
    "LICENSE"
  ],
  "directories": {
    "lib": "js",
    "test": "tests",
    "doc": "docs"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "security": {
    "advisories": {
      "ignore": []
    }
  },
  "performance": {
    "budgets": {
      "bundle": "250kb",
      "assets": "500kb",
      "total": "1mb"
    }
  },
  "quality": {
    "coverage": {
      "minimum": 80
    },
    "complexity": {
      "maximum": 10
    }
  }
}